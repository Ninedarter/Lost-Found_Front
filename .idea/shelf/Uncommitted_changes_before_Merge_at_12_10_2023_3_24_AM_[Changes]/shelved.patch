\n            </div>\r\n            <span>\r\n                <Button label=\"Submit\"\r\n                        onClick={(e) => handleSubmit(e)}\r\n                        style={{width:\"100%\"}}\r\n                        disabled={submitted}\r\n                />\r\n                </span>\r\n            </div>\r\n        </Card>\r\n    )\r\n}\r\n\r\n\r\nexport default LoginForm
